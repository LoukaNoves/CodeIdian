{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "types": ["node", "obsidian"],
    "outDir": "./dist"
  },
  "include": [
    "src/**/*.ts",
    "main.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
